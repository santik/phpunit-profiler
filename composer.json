{
    "name": "santik/phpunit-profiler",
    "description": "Phpunit listener for profiling tests",
    "license": "proprietary",
    "authors": [
        {
              "name": "Alexander Fedortets",
              "email": "fedorets.alex@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Santik\\PhpunitProfiler\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Santik\\PhpunitProfiler\\": ["tests/unit/"]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
